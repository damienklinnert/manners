{
  "white": true,
  "bitwise": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "plusplus": false,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": "vars",
  "strict": true,
  "trailing": true,
  "maxparams": 5,
  "maxdepth": 5,
  "maxstatements": 40,
  "maxlen": 100,
  "predef": [
    "suite",
    "setup",
    "test",
    "teardown",
    "describe",
    "it",
    "beforeEach",
    "before"
  ],
  "node": true,
  "esnext": true
}
